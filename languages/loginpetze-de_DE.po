msgstr "Name: [username]"

#: includes/function-build-default-mailbody.php:33
msgid "Blog: [blogname]"
msgstr "Blog: [blogname]"

#: includes/function-build-default-mailbody.php:37
msgid "Time: [date] at [time]"
msgstr "Zeit: [date] um [time] Uhr"

#: includes/function-build-default-mailbody.php:43
msgid "Greetings"
msgstr "Viele Grüße"

#: includes/function-build-default-mailbody.php:46
#: includes/function-options-page.php:29
